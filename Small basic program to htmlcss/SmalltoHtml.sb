'start window
Sub SetupStartWindow
  GraphicsWindow.Title = "Create Html File Setup"
  GraphicsWindow.BrushColor = "black"
  htmlNameText = Shapes.AddText("Html File Name")
  htmlNameText2 = Shapes.AddText("Html File 2 Name")
  cssNameText = Shapes.AddText("Css File Name")
  Shapes.Move(htmlNameText,280,titleRegion+85)
  Shapes.Move(htmlNameText2,270,titleRegion+120)
  Shapes.Move(cssNameText,280,titleRegion+155)
  GraphicsWindow.BrushColor = "gray"
  htmlNameBox = Controls.AddTextBox(380,titleRegion+80)
  htmlNameBox2 = Controls.AddTextBox(380,titleRegion+115)
  cssNameBox = Controls.AddTextBox(380,titleRegion+150)
  Controls.SetSize(htmlNameBox,150,30)
  Controls.SetSize(htmlNameBox2,150,30)
  Controls.SetSize(cssNameBox,150,30)
  GraphicsWindow.FontSize = 90
  GraphicsWindow.BrushColor="black"
  CreateButton = Controls.AddButton("Create",315,titleRegion+280)
  GraphicsWindow.FontSize = 15
  GraphicsWindow.BrushColor="gray"
  ViewHtml = Shapes.AddText("To view Html File, Open C:\Small Basic\SmallBasicWebsite\ Your File Name")
  Shapes.Move(ViewHtml,160,titleRegion+200)
EndSub


